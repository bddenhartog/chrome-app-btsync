{
  "manifest_version": 2,
  "name": "BTsync GUI",
  "short_name": "Sync GUI",
  "description": "Simple GUI usage for BitTorrent Sync.",
  "version": "0.1.4",
  
  "app": {
    "background": {
      "scripts": ["app/background.js"]
    }
  },

  "permissions": [
    "webview"
  ],
  
  "icons": { 
    "16": "images/btsync-16.png", 
    "32": "images/btsync-32.png",
    "128": "images/btsync-128.png"
  }
}
